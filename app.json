{
  "expo": {
    "name": "My Curated Haven",
    "slug": "my-curated-haven",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mycuratedhaven",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "Your trusted companion for the parenting journey. Get personalized guidance, track milestones, and connect with expert advice.",
    "primaryColor": "#D4635A",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FDF7F3"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pratikn07.mycuratedhaven",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses camera to capture photos for sharing parenting moments.",
        "NSMicrophoneUsageDescription": "This app uses microphone for voice messages and questions.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.pratikn07.mycuratedhaven",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#FDF7F3"
      },
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "21e5e981-1fed-4d12-b6cd-c83146326e16"
      }
    }
  }
}
