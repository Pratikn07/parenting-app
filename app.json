{
  "expo": {
    "name": "Parenting App",
    "slug": "parenting-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "parenting-app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "description": "Your trusted companion for the parenting journey. Get personalized guidance, track milestones, and connect with expert advice.",
    "primaryColor": "#D4635A",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FDF7F3"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pratikn07.mycuratedhaven",
      "buildNumber": "1"
    },
    "android": {
      "package": "com.pratikn07.mycuratedhaven",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#FDF7F3"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
}